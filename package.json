{
  "name": "openinapp-backend-task",
  "version": "1.0.0",
  "description": "",
  "main": "/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=development nodemon src/index.js --watch .",
    "start": " nodemon src/index.js --watch .",
    "lint": "npx prettier --check .",
    "format": "npx prettier --write .",
    "prepare": "npx husky install",
    "db:migrate:up": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed:all": "npx sequelize db:seed:all",
    "db:seed:undo:all": "npx sequelize db:seed:undo:all",
    "db:reset": "NODE_ENV=development sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "dev:generate:modelEnums": "NODE_ENV=development node src/utils/generateModelEnums.js",
    "prod:generate:modelEnums": "NODE_ENV=production node src/utils/generateModelEnums.js"
  },
  "author": "Bharat Kumar",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "node-cron": "^3.0.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "sequelize": "^6.35.2",
    "twilio": "^4.21.0",
    "winston": "^3.11.0"
  }
